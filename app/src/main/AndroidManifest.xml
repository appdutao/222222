<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dutao.listview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dutao.listview.FirstActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.dutao.listview.ListContentActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.dutao.main.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dutao.btten.BttenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dutao.viewpager.ViewPagerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dutao.listview.ActionBarActivity"
            android:label="@string/title_activity_action_bar" android:theme="@android:style/Theme.Light.NoTitleBar">
        </activity>
    </application>

</manifest>